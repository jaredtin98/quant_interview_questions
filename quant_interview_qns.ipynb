                "import math\r\n",
                "def pascalLayer(n):\r\n",
                "    \r\n",
                "    def binomialCoeff(line, i):\r\n",
                "        return math.comb(line, i)\r\n",
                "    \r\n",
                "    for line in range(0, n):\r\n",
                "        for i in range(0, line + 1):\r\n",
                "            print(binomialCoeff(line, i), \" \", end = \"\")\r\n",
                "        print()\r\n",
                "\r\n",
                "pascalLayer(4)"
            ],
